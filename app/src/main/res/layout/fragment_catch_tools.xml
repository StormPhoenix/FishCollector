<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <!--<import type="com.stormphoenix.fishcollector.mvp.model.beans.CatchTools" />-->

        <!--<variable-->
            <!--name="catchToolsBean"-->
            <!--type="CatchTools" />-->
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="10dp">

        <TextView
            android:id="@+id/tv_net_name"
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:layout_marginTop="10dp"
            android:gravity="center_vertical"
            android:text=" 名称    " />


        <EditText
            android:id="@+id/net_name"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@id/tv_net_name"
            android:hint=" 名称"
            android:paddingLeft="10dp"
            android:singleLine="true"/>
            <!--android:text="@={catchToolsBean.name}" />-->

        <LinearLayout
            android:id="@+id/net_layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/net_name"
            android:layout_marginTop="5dp"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_net_type"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:text=" 网型    " />

                <EditText
                    android:id="@+id/net_type"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/tv_net_type"
                    android:layout_weight="1"
                    android:hint=" 网型"
                    android:paddingLeft="10dp"
                    android:singleLine="true"/>
                    <!--android:text="@={catchToolsBean.netsModel}" />-->
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_net_area"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:text=" 网口面积    " />

                <EditText
                    android:id="@+id/net_area"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/tv_net_area"
                    android:hint=" 网口面积"
                    android:paddingLeft="10dp"
                    android:singleLine="true"/>
                    <!--android:text="@={catchToolsBean.netMouthArea}" />-->
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/net_layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/net_layout1"
            android:layout_marginTop="5dp"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_net_angle"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:text=" 网口倾角    " />


                <EditText
                    android:id="@+id/net_angle"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/tv_net_angle"
                    android:layout_weight="1"
                    android:hint=" 网口倾角"
                    android:paddingLeft="10dp"
                    android:singleLine="true"/>
                    <!--android:text="@={catchToolsBean.netMouthDip}" />-->
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_net_velocity"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:text=" 网口流速    " />


                <EditText
                    android:id="@+id/net_velocity"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/tv_net_velocity"
                    android:hint=" 网口流速"
                    android:paddingLeft="10dp"
                    android:singleLine="true"/>
                    <!--android:text="@={catchToolsBean.netMouthVelocity}" />-->
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/net_layout3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/net_layout2"
            android:layout_marginTop="5dp"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_start_time"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:text=" 开始时间    " />


                <EditText
                    android:id="@+id/start_time"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/tv_start_time"
                    android:layout_weight="1"
                    android:hint=" 开始时间"
                    android:paddingLeft="10dp"
                    android:singleLine="true"/>
                    <!--android:text="@={catchToolsBean.startTime}" />-->
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_end_time"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:layout_marginTop="10dp"
                    android:gravity="center_vertical"
                    android:text=" 结束时间    " />

                <EditText
                    android:id="@+id/end_time"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="10dp"
                    android:layout_toRightOf="@id/tv_end_time"
                    android:hint=" 结束时间"
                    android:paddingLeft="10dp"
                    android:singleLine="true"/>
                    <!--android:text="@={catchToolsBean.endTime}" />-->
            </RelativeLayout>
        </LinearLayout>

        <GridLayout
            android:id="@+id/cont_net_add_pic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/net_layout3"
            android:layout_marginTop="10dp"
            android:columnCount="4"
            android:padding="3dp">

        </GridLayout>

    </RelativeLayout>
</layout>